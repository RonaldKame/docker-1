#!/bin/bash

pushd editor-de-servicos > /dev/null
docker build -t=servicosgovbr/editor-de-servicos .
popd > /dev/null

pushd portal-de-servicos > /dev/null
docker build -t=servicosgovbr/portal-de-servicos .
popd > /dev/null

pushd balanceador > /dev/null
docker build -t=servicosgovbr/balanceador .
popd > /dev/null
